bool isalnum(int c)
{
	return (bool)(ACSUtils_AsciiTypes[c] & ACSUTILS_ISALNUM);
}

 
bool isalpha(int c)
{
	return (bool)(ACSUtils_AsciiTypes[c] & ACSUTILS_ISALPHA);
}

 

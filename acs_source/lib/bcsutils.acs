bool isupper(int c)
{
	return (bool)(ACSUtils_AsciiTypes[c] & ACSUTILS_ISUPPER);
}

 
bool isxdigit(int c)
{
	return (bool)(ACSUtils_AsciiTypes[c] & ACSUTILS_ISXDIGIT);
}

 
bool isascii(int c)
{
	return c >= 0 && c < 128;
}

 
int toupper(int c)
{
	if (islower(c))
		return c - 32;
	return c;
}

 
int tolower(int c)
{
	if (isupper(c))
		return c + 32;
	return c;
}


